#. [dummy], id=feeding_easy
#: Legend_of_the_Invincibles-master/utils/abilities.cfg:2127
msgid "feeding"
msgstr "alimenta"

#. [dummy], id=feeding_easy
#: Legend_of_the_Invincibles-master/utils/abilities.cfg:2128
msgid "female^feeding"
